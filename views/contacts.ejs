<table>
    <thead>
        <tr>
            <th>First name</th>
            <th>Last name</th>
        </tr>
    </thead>
    <tbody>
        <% contacts.forEach(i => { %>
        <tr>
            <td><%= i.firstname %></td>
            <td><%= i.lastname %></td>
        </tr>
        <% }) %>
    </tbody>
</table>